{
  "mcpServers": {
    "Framelink Figma MCP": {
      "command": "bunx",
      "args": [
        "-y",
        "figma-developer-mcp",
        "--figma-api-key=${YOUR-KEY}",
        "--stdio"
      ]
    },
    "blender": {
      "command": "uvx",
      "args": ["blender-mcp"]
    },
    "github": {
      "command": "bunx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${YOUR_TOKEN}"
      }
    }
  }
}
