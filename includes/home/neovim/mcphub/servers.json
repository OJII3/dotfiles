{
  "mcpServers": {
    "figma": {
      "url": "http://127.0.0.1:3845/mcp"
    },
    "blender": {
      "command": "uvx",
      "args": ["blender-mcp"]
    },
    "github": {
      "command": "bunx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${YOUR_TOKEN}"
      }
    },
    "git": {
      "command": "uv",
      "args": ["tool", "run", "mcp-server-git"]
    },
    "playwright": {
      "command": "bunx",
      "args": ["@playwright/mcp@latest"]
    },
    "mcp-deepwiki": {
      "command": "bunx",
      "args": ["-y", "mcp-deepwiki@latest"]
    }
  }
}
